    "browser": {
      "type": "select_or_input",
      "label": "🌐 Browser Type",
      "options": ["chrome", "chromium", "firefox", "safari", "msedge"],
      "allow_custom": true,
      "placeholder": "chrome, firefox, etc."
    },
    "SET_BROWSER": {
      "type": "select_or_input",
      "label": "🌐 Browser",
      "options": ["chrome", "chromium", "firefox", "safari", "msedge"],
      "allow_custom": true,
      "placeholder": "chrome, firefox, etc."
    },
    "action": {
      "type": "select_or_input",
      "label": "⚡ Action Type",
      "options": ["OK", "Cancel", "accept", "dismiss"],
      "allow_custom": true,
