      "allow_custom": true,
      "placeholder": "Select or type custom button name"
    },
    "modal_dialog_btn": {
      "type": "select_or_input",
      "label": "🔘 Modal Button",
      "options": ["", "Yes", "No", "OK", "Close", "Cancel", "Confirm"],
      "allow_custom": true,
      "placeholder": "Leave empty to bypass modal"
    },
    "assertion": {
      "type": "select_or_input",
      "label": "✓ Assertion Type",
      "options": ["equal", "should be", "should not be", "contains", "not contains", "==", "!="],
      "allow_custom": true,
      "placeholder": "Select or type custom assertion"
