      "type": "select_or_input",
      "label": "🔢 Decimal Points",
      "options": ["0", "1", "2", "3", "4"],
      "allow_custom": true,
      "placeholder": "Number of decimal places"
    },
    "is_checkboxtype": {
      "type": "boolean",
      "label": "☑️ Is Checkbox Type",
      "default": "false"
    },
    "is_antdesign": {
      "type": "boolean",
      "label": "🎨 Is Ant Design",
      "default": "false"
    },
    "is_switchtype": {
      "type": "boolean",
      "label": "🔀 Is Switch Type",
      "default": "false"
    },
    "ignorcase": {
      "type": "boolean",
      "label": "📝 Ignore Case",
      "default": "false"
    },
    "antdesign": {
      "type": "boolean",
      "label": "🎨 Ant Design",
      "default": "false"
    },
    "is_skipfield": {
      "type": "boolean",
      "label": "⏭️ Skip Field",
      "default": "false"
    },
    "escape_action": {
      "type": "boolean",
      "label": "🚫 Escape Action",
      "default": "false"
    },
    "custom_locator": {
      "type": "boolean",
      "label": "🎯 Custom Locator",
      "default": "false"
    },
    "use_comma": {
      "type": "boolean",
      "label": "📊 Use Comma",
      "default": "true"
    },
    "is_upper": {
      "type": "select_or_input",
      "label": "🔤 Case Type",
      "options": ["true", "false"],
      "allow_custom": true,
      "placeholder": "true or false"
    },
    "is_ant_design": {
      "type": "boolean",
      "label": "🎨 Ant Design Mode",
      "default": "false"
    }
  },
  "patterns": {
    "password": {
      "type": "password",
      "label": "🔒 Password",
      "placeholder": "Enter password or ${VAR_PASSWORD}"
    },
    "username": {
      "type": "text",
      "label": "👤 Username",
      "placeholder": "Enter username or ${VAR_USERNAME}",
      "suggestions": ["${VAR_USERNAME}", "${GLOBAL_USER1}", "admin"]
    },
    "email": {
      "type": "text",
      "label": "📧 Email",
      "placeholder": "user@example.com or ${VAR_EMAIL}"
    },
    "url": {
      "type": "text",
      "label": "🔗 URL",
      "placeholder": "https://example.com or ${URLTEST}"
    },
    "path": {
      "type": "text",
      "label": "📁 Path",
      "placeholder": "${EXECDIR}/path/to/file"
    },
    "jsonpath": {
      "type": "text",
      "label": "🗂️ JSON Path",
      "placeholder": "$.data.items[0].name"
    }
