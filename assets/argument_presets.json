    "timeout": {
      "type": "select_or_input",
      "label": "⏱️ Timeout",
      "options": ["5s", "10s", "20s", "30s", "40s", "60s", "1min", "2min"],
      "allow_custom": true,
      "placeholder": "e.g., 30s or 1min"
