      "type": "select_or_input",
      "label": "✓ Assert",
      "options": ["equal", "should be", "should not be", "contains", "not contains"],
      "allow_custom": true,
      "placeholder": "Select or type custom assertion"
    },
    "state": {
      "type": "select_or_input",
      "label": "🎭 Element State",
      "options": ["visible", "hidden", "enabled", "disabled", "attached", "detached", "editable", "readonly"],
      "allow_custom": true,
      "placeholder": "Select or type custom state"
