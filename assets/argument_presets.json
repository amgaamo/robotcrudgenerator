    "delaytime": {
      "type": "select_or_input",
      "label": "⏱️ Delay Time",
      "options": ["500ms", "1000ms", "1200ms", "1500ms", "2000ms"],
      "allow_custom": true,
      "placeholder": "e.g., 1200ms"
